{
  "name": "sleepy-pine",
  "version": "1.0.0",
  "author": "PineAG",
  "license": "MIT",
  "dependencies": {
    "chromium-revision": "^1.0.0",
    "parcel": "^2.0.0-beta.2",
    "pug": "^3.0.2",
    "sass": "^1.35.2"
  },
  "scripts": {
    "serve": "parcel serve --host 0.0.0.0 index.pug",
    "build": "parcel build --dist-dir dist --no-minify index.pug",
    "build-static": "parcel build --dist-dir dist --no-minify --public-url '.' index.pug",
    "build-gh-pages": "parcel build --dist-dir dist --no-minify --public-url '/byebye-page' index.pug"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "2.0.0-beta.2",
    "@parcel/transformer-sass": "2.0.0-beta.2"
  }
}
